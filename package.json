{
  "name": "descriptor-merge",
  "version": "0.1.0-0",
  "description": "merge or extend objects with descriptors",
  "main": "index.js",
  "engines": {
    "node": ">=0.8.0"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "mocha": "latest"
  },
  "scripts": {
    "test": "mocha test/*.mocha.js",
    "cover": "istanbul cover _mocha --report lcov --report text -- -R dot --check-leaks test/*.mocha.js",
    "lint": "jshint --show-non-errors *.js */*.js",
    "doc": "jsdoc -d doc -c jsdoc.json index.js lib/*.js",
    "clean": "rm -rf doc coverage"
  },
  "repository": {
    "type": "git",
    "url": "github.com/commenthol/descriptor-merge"
  },
  "keywords": [
    "object",
    "merge",
    "extend",
    "descriptor"
  ],
  "author": "commenthol <commenthol@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commenthol/descriptor-merge/issues"
  },
  "homepage": "https://github.com/commenthol/descriptor-merge"
}
